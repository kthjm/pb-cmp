{
  "name": "",
  "version": "0.0.0",
  "author": "kthjm <toxictoxer@gmail.com>",
  "repository": "kthjm/{name}",
  "license": "MIT",
  "description": "",
  "keywords": [],
  "main": "dist/{name}.cjs.js",
  "jsnext:main": "dist/{name}.es.js",
  "module": "dist/{name}.es.js",
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "nyc": {
    "require": ["babel-register", "jsdom-global/register"],
    "include": ["src"],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "type:stub": "flow-typed create-stub",
    "type:add": "flow-typed install",
    "prestart":
      "cross-env BABEL_ENV=WDS node -r babel-register ./wds/prestart.js",
    "start":
      "cross-env BABEL_ENV=WDS webpack-dev-server --config ./wds/webpack.config.babel.js",
    "pretest": "flow",
    "test": "cross-env NODE_ENV=TEST nyc mocha",
    "prebuild": "yarn test",
    "build": "cross-env NODE_ENV=BUILD node rollup.config.all.js",
    "postbuild": "run-s format",
    "format": "prettier --write **/*.js package.json",
    "coverage": "nyc report --reporter=lcov && codecov"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^2.0.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "execa": "^0.9.0",
    "flow-bin": "^0.65.0",
    "flow-typed": "^2.3.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.4.1",
    "path-exists": "^3.0.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rewire": "^3.0.2",
    "rollup": "^0.55.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^4.2.2",
    "uglify-es": "^3.3.9",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  }
}
